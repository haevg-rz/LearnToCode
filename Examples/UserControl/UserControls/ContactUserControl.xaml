<UserControl x:Class="UserControl.UserControls.ContactUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800" Background="Beige">

    <Border BorderBrush="Gray" BorderThickness="0.5" Margin="5" Padding="2.5"
            d:DataContext="{StaticResource DesignDataEmployee}">
        <StackPanel>
            <TextBlock Text="{Binding Name}" />
            <TextBlock Text="{Binding BirthDate}" />
        </StackPanel>
    </Border>
</UserControl>